@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.videogamesAwards.org/videogamesAwards2020> .

<http://www.videogamesAwards.org/videogamesAwards2020> rdf:type owl:Ontology ;
                                                        <http://purl.org/dc/elements/1.1/creator> "Francesco Di Sario"@en ,
                                                                                                  "Giuseppe Ignone"@en ;
                                                        <http://purl.org/dc/elements/1.1/date> "04-01-2021"@en ;
                                                        <http://purl.org/dc/elements/1.1/description> "Questa ontologia ha l'obiettivo di fornire una panoramica generale sui videogames con particolare riferimento ai risultati ottenuti da ciascun gioco in termini di recensioni svolte sia dalla critica esperta che dall'utenza (comuni videogiocatori) e premi ottenuti, assegnati dalle varie redazioni che lavorano per le testate giornalistiche."@it ;
                                                        <http://purl.org/dc/elements/1.1/label> "videogamesAwards2020"@en ;
                                                        <http://purl.org/dc/elements/1.1/title> "videogames critics and awards 2020"@en ;
                                                        <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements> "What are the elements in this set?"^^xsd:string ,
                                                                                                                                                  "What is the cardinality (size) of this set?"^^xsd:string ,
                                                                                                                                                  "What things are contained in this collection (community, collective)? What collections this thing is member of?" ;
                                                        <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#extractedFrom> "http://swan.mindinformatics.org/ontologies/1.2/collections.owl"^^xsd:string ,
                                                                                                                                             "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl" ;
                                                        <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasAuthor> "Aldo Gangemi" ,
                                                                                                                                         "PaoloCiccarese"^^xsd:string ;
                                                        <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasConsequences> "Collections and their members can be associated. Time-indexed membership cannot be represented though (you need a situation-based pattern)." ;
                                                        <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasIntent> "To model sets of things (elements). A Set is a collection that cannot contain duplicate elements. "^^xsd:string ,
                                                                                                                                         "To represent domain (not set theory) membership." ;
                                                        <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#reengineeredFrom> "http://swan.mindinformatics.org/ontologies/1.2/collections.owl"^^xsd:string ;
                                                        <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#relatedCPs> "TimeIndexedMembership" ,
                                                                                                                                          "http://www.ontologydesignpatterns.org/cp/owl/bag.owl"^^xsd:string ,
                                                                                                                                          "http://www.ontologydesignpatterns.org/cp/owl/list.owl"^^xsd:string ;
                                                        <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#scenarios> "My saxophone collection includes a Mark VI tenor, a Balanced Action alto, and a Conn Transitional bari." ;
                                                        :vann:preferredNamespacePrefix "vgaw"@en ;
                                                        :vann:preferredNamespaceUri "http://www.videogamesAwards.org/videogamesAwards2020"@en ;
                                                        rdfs:comment "A Set is a collection that cannot contain duplicate elements. A Set is expressed by linking to it directly all the members (elements), multiple identical values of members (elements) will be eliminated because by default they are treated as a set."^^xsd:string ,
                                                                     """The collection entity pattern. 
It is extracted from DOLCE-UltraLite by partial clone of elements.""" ;
                                                        rdfs:label "collection entity" ,
                                                                   "membership" ;
                                                        owl:versionInfo "1.1 added rdfs:isDefinedBy for all named entities"^^xsd:string ,
                                                                        "Created by Aldo Gangemi and Valentina Presutti"^^xsd:string ,
                                                                        "Created with TopBraid Composer"^^xsd:string .

